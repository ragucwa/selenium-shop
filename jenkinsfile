pipeline{
    agent any
    stages{
        stage('Build docker image'){
            steps{
                script{
                    app = docker.build('guttekkk/selenium-training')
                }
            }
        }
        stage('Push docker image to docker hub'){
            steps{
                script{
                    docker.withRegistry('', 'docker_hub') {
                        app.push("latest")
                    }
                }
            }
        }
    }
}
